.top{
    position: absolute;
    width:100%;
    top:10rpx;
    display: flex;
    flex-flow:wrap;
    z-index: 10;
    color:white;
    justify-content:space-around;
  }
  page{
    background:rgb(14, 13, 13);
  }
  view{
    font-size: 30rpx;
  }
  .bottom{
    position: absolute;
    width:100%;
    bottom:50rpx;
    display: flex;
    z-index: 10;
    align-items: center;
    flex-wrap:wrap;
    height:100rpx;
  }
  .bottom .btnBox{
    display: inline-block;
    font-size:28rpx;
    z-index:2;
    height:60rpx;
    /* line-height:30rpx; */
    width:22%;
    margin:0rpx 1%;
    text-align:center;
    color:#fff;
  }
  .cancelBut{
    line-height:60rpx;
  }
  /* .btnBox1{
    width:10%;
  } */
  .imageBox image{
     width:40rpx;
     height:40rpx;
     margin-top:10rpx;
  }
  .btnColor{
    line-height:30rpx;
    background:var(--themeColor);
  }
  .input{
    display: flex;
    height: 50rpx;
    width:50%;
  }
  .input>.label{
    min-width:150rpx;
    font-size: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
  }
  .input>input{
    margin-left:10rpx;
    text-align: center;
    max-width:160rpx;
    border:1px solid rgb(255, 255, 255);
    height: 50rpx;
    line-height: 50rpx;
    min-height: 50rpx;
    box-sizing: border-box;
  }